#include <iostream>
#include <stdio.h>
 
using namespace std;
    
int main () {
    cout << "Введите число n" << endl;
  int n,i,p;
  cin>>n;
  int a[n+1];
  
  for (i=2; i<=n; i++) {
    a[i]=i;
  }
 
  cout << "Все простые числа до n " << endl;
 
  for(i = 2;i<=n;i++)
  {
    if(a[i] != 0)
    {
        cout << a[i] << endl;
        for(p = 2 * i; p <= n ; p += i)
        {
            a[p] = 0;
        }
    }
  }
  getchar();
  return 0;
 
}
